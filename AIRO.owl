@prefix : <https://w3id.org/airo#> .
@prefix dc: <http://purl.org/dc/elements/1.1/> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix airo: <https://w3id.org/airo#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix terms: <http://purl.org/dc/terms/> .
@base <https://w3id.org/airo> .

<https://w3id.org/airo> rdf:type owl:Ontology ;
                         terms:creator "Delaram Golpayegani"^^xsd:string ;
                         terms:description "The ontology represents AI risk concepts and relations based on the AI Act draft and ISO 31000 standard series."@en ;
                         terms:license <https://creativecommons.org/licenses/by/4.0/> ;
                         terms:publisher "ADAPT Centre, Trinity College Dublin"^^xsd:string ;
                         rdfs:label "AI Risk Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
terms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
terms:description rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
terms:license rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
terms:publisher rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/airo#eliminatesEvent
airo:eliminatesEvent rdf:type owl:ObjectProperty ;
                     rdfs:subPropertyOf airo:modifiesEvent ;
                     rdfs:label "eliminates event"@en .


###  https://w3id.org/airo#hasComponent
airo:hasComponent rdf:type owl:ObjectProperty ;
                  owl:inverseOf airo:isPartOfAiSystem ;
                  rdfs:domain airo:AiSystem ;
                  rdfs:range airo:SystemComponent ;
                  rdfs:label "has component"@en .


###  https://w3id.org/airo#hasConsequence
airo:hasConsequence rdf:type owl:ObjectProperty ;
                    rdfs:domain airo:Risk ;
                    rdfs:range airo:Consequence ;
                    rdfs:label "has consequence"@en .


###  https://w3id.org/airo#hasExtentOfUseIndicator
airo:hasExtentOfUseIndicator rdf:type owl:ObjectProperty ;
                             rdfs:domain airo:AiSystem ;
                             rdfs:range airo:ExtentOfUse .


###  https://w3id.org/airo#hasImpact
airo:hasImpact rdf:type owl:ObjectProperty ;
               owl:inverseOf airo:isImpactOf ;
               rdfs:domain airo:Consequence ;
               rdfs:range airo:Impact ;
               rdfs:label "has impact"@en .


###  https://w3id.org/airo#hasImpactOnArea
airo:hasImpactOnArea rdf:type owl:ObjectProperty ;
                     rdfs:domain airo:Impact ;
                     rdfs:range airo:Area ;
                     rdfs:label "has impact on area"@en .


###  https://w3id.org/airo#hasImpactOnStakeholder
airo:hasImpactOnStakeholder rdf:type owl:ObjectProperty ;
                            rdfs:domain airo:Impact ;
                            rdfs:range airo:Stakeholder ;
                            rdfs:label "has impact on stakeholder"@en .


###  https://w3id.org/airo#hasLikelihood
airo:hasLikelihood rdf:type owl:ObjectProperty ;
                   rdfs:domain airo:Event ;
                   rdfs:range airo:LikelihoodRate ;
                   rdfs:label "Indicates the probability of occurrence of an event."@en ,
                              "has likelihood"@en .


###  https://w3id.org/airo#hasPerformanceIndicator
airo:hasPerformanceIndicator rdf:type owl:ObjectProperty ;
                             rdfs:domain airo:AiSystem ;
                             rdfs:range airo:Performace .


###  https://w3id.org/airo#hasPurpose
airo:hasPurpose rdf:type owl:ObjectProperty ;
                owl:inverseOf airo:isPurposeOf ;
                rdfs:domain airo:AiSystem ;
                rdfs:range airo:Purpose ;
                rdfs:label "Indicates the intended purpose of an AI system."@en ,
                           "has purpose"@en .


###  https://w3id.org/airo#hasRisk
airo:hasRisk rdf:type owl:ObjectProperty ;
             owl:inverseOf airo:isRiskAssociatedWith ;
             rdfs:range airo:Risk ;
             rdfs:label "has risk"@en .


###  https://w3id.org/airo#hasRiskSource
airo:hasRiskSource rdf:type owl:ObjectProperty ;
                   owl:inverseOf airo:isRiskSourceFor ;
                   rdfs:domain airo:Risk ;
                   rdfs:range airo:Source ;
                   rdfs:label "has risk source"@en .


###  https://w3id.org/airo#hasSeverity
airo:hasSeverity rdf:type owl:ObjectProperty ;
                 rdfs:domain [ rdf:type owl:Class ;
                               owl:unionOf ( airo:Consequence
                                             airo:Impact
                                           )
                             ] ;
                 rdfs:range airo:Severity ;
                 rdfs:label "has severity"@en .


###  https://w3id.org/airo#hasStakeholder
airo:hasStakeholder rdf:type owl:ObjectProperty ;
                    owl:inverseOf airo:isStakeholderOfSystem ;
                    rdfs:domain airo:AiSystem ;
                    rdfs:range airo:Stakeholder ;
                    rdfs:comment "Indicates stakeholders of an AI system"@en ;
                    rdfs:label "has stakeholder"@en .


###  https://w3id.org/airo#hasVulnerability
airo:hasVulnerability rdf:type owl:ObjectProperty ;
                      rdfs:domain airo:AiSystem ;
                      rdfs:range airo:vulnerability ;
                      rdfs:comment "Indicates vulnerabilities of an AI system."@en ;
                      rdfs:label "has vulnerability"@en .


###  https://w3id.org/airo#isConsequenceOf
airo:isConsequenceOf rdf:type owl:ObjectProperty ;
                     rdfs:domain airo:Consequence ;
                     rdfs:range airo:Risk ;
                     rdfs:label "is consequence of"@en .


###  https://w3id.org/airo#isContextOfUseFor
airo:isContextOfUseFor rdf:type owl:ObjectProperty ;
                       owl:inverseOf airo:isUsedInContext ;
                       rdfs:domain airo:Context ;
                       rdfs:range airo:AiSystem ;
                       rdfs:label "is context of use for"@en .


###  https://w3id.org/airo#isFollowedByControl
airo:isFollowedByControl rdf:type owl:ObjectProperty ;
                         rdfs:domain airo:Measure ;
                         rdfs:range airo:Measure ;
                         rdfs:label "is followed by control"@en .


###  https://w3id.org/airo#isImpactOf
airo:isImpactOf rdf:type owl:ObjectProperty ;
                rdfs:domain airo:Impact ;
                rdfs:range airo:Consequence ;
                rdfs:label "is impact of"@en .


###  https://w3id.org/airo#isInVulnerablePosition
airo:isInVulnerablePosition rdf:type owl:ObjectProperty ;
                            rdfs:domain [ rdf:type owl:Class ;
                                          owl:unionOf ( airo:Group
                                                        airo:Person
                                                      )
                                        ] ;
                            rdfs:range airo:VulnerablePosition ;
                            rdfs:label "is in vulnerable position"@en .


###  https://w3id.org/airo#isPartOfAiSystem
airo:isPartOfAiSystem rdf:type owl:ObjectProperty ;
                      rdfs:domain airo:SystemComponent ;
                      rdfs:range airo:AiSystem ;
                      rdfs:comment "Indicates that a component is part of an AI system."@en ;
                      rdfs:label "is part of ai system"@en .


###  https://w3id.org/airo#isPartOfControl
airo:isPartOfControl rdf:type owl:ObjectProperty ;
                     rdfs:domain airo:PartialControl ;
                     rdfs:range airo:Measure ;
                     rdfs:label "is part of control"@en .


###  https://w3id.org/airo#isPurposeOf
airo:isPurposeOf rdf:type owl:ObjectProperty ;
                 rdfs:domain airo:Purpose ;
                 rdfs:range airo:AiSystem ;
                 rdfs:label "is purpose of"@en .


###  https://w3id.org/airo#isRiskAssociatedWith
airo:isRiskAssociatedWith rdf:type owl:ObjectProperty ;
                          rdfs:domain airo:Risk ;
                          rdfs:label "is risk associated with"@en .


###  https://w3id.org/airo#isRiskSourceFor
airo:isRiskSourceFor rdf:type owl:ObjectProperty ;
                     rdfs:domain airo:Source ;
                     rdfs:range airo:Risk ;
                     rdfs:label "is risk source for"@en .


###  https://w3id.org/airo#isStakeholderOfSystem
airo:isStakeholderOfSystem rdf:type owl:ObjectProperty ;
                           rdfs:domain airo:Stakeholder ;
                           rdfs:range airo:AiSystem ;
                           rdfs:label "is stakeholder of system"@en .


###  https://w3id.org/airo#isUsedInContext
airo:isUsedInContext rdf:type owl:ObjectProperty ;
                     rdfs:comment "Indicates the context the AI system is intended to be used in."@en ;
                     rdfs:label "is used in context"@en .


###  https://w3id.org/airo#mitigatesEvent
airo:mitigatesEvent rdf:type owl:ObjectProperty ;
                    rdfs:subPropertyOf airo:modifiesEvent ;
                    rdfs:label "mitigates event"@en .


###  https://w3id.org/airo#modifiesEvent
airo:modifiesEvent rdf:type owl:ObjectProperty ;
                   rdfs:domain airo:Measure ;
                   rdfs:range airo:Event ;
                   rdfs:label "modifies event"@en .


###  https://w3id.org/airo#producesOutput
airo:producesOutput rdf:type owl:ObjectProperty ;
                    rdfs:domain airo:AiSystem ;
                    rdfs:range airo:Output ;
                    rdfs:label "produces output"@en .


###  https://w3id.org/airo#usesTechnique
airo:usesTechnique rdf:type owl:ObjectProperty ;
                   rdfs:domain airo:AiSystem ;
                   rdfs:range airo:AiTechnique ;
                   rdfs:comment "Indicates the AI techniques used in an AI system."@en ;
                   rdfs:label "uses technique"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/airo#Accuracy
airo:Accuracy rdf:type owl:Class ;
              rdfs:subClassOf airo:Indicator .


###  https://w3id.org/airo#AccuracyOFData
airo:AccuracyOFData rdf:type owl:Class ;
                    rdfs:subClassOf airo:Accuracy ;
                    rdfs:comment "Refers to degree to which data has attributes that correctly represent the true value of the intended attribute of a concept or event in a specific context of use. [ISO 22989, 3.4.3]"@en ;
                    rdfs:label "Accuracy Of Data"@en .


###  https://w3id.org/airo#AccuracyOfAiSystem
airo:AccuracyOfAiSystem rdf:type owl:Class ;
                        rdfs:subClassOf airo:Accuracy ;
                        rdfs:comment "degree to which a product or system provides the correct results with the needed degree of precision. [ISO 22989, 3.4.4]"@en ;
                        rdfs:label "Accuracy Of AI System"@en .


###  https://w3id.org/airo#AdministrationOfJustice
airo:AdministrationOfJustice rdf:type owl:Class ;
                             rdfs:subClassOf airo:Context ;
                             rdfs:label "Administration Of Justice"@en .


###  https://w3id.org/airo#AdversarialAttack
airo:AdversarialAttack rdf:type owl:Class ;
                       rdfs:subClassOf airo:Cyberattack ;
                       rdfs:comment "inputs designed to cause the model to make a mistake"@en ;
                       rdfs:label "Adversarial Attack"@en .


###  https://w3id.org/airo#AiSystem
airo:AiSystem rdf:type owl:Class ;
              rdfs:comment "Represents a software that is developed with one or more of the AI Approaches and can, for a given set of human-defined objectives, generate outputs such as content, predictions, recommendations, or decisions influencing the environments they interact with. [Source: AI Act, Art 3(1)]"@en ;
              rdfs:label "AI System"@en .


###  https://w3id.org/airo#AiTechnique
airo:AiTechnique rdf:type owl:Class ;
                 rdfs:comment "Refers to the main approaches and techniques used for development of AI systems."@en ;
                 rdfs:label "AI Technique"@en .


###  https://w3id.org/airo#Area
airo:Area rdf:type owl:Class ;
          rdfs:label "Area"@en .


###  https://w3id.org/airo#AutomationBias
airo:AutomationBias rdf:type owl:Class ;
                    rdfs:subClassOf airo:Consequence ;
                    rdfs:comment "tendency of automatically relying or over-relying on the output produced by an AI system."@en .


###  https://w3id.org/airo#Bias
airo:Bias rdf:type owl:Class ;
          rdfs:subClassOf airo:Consequence ;
          rdfs:comment "systematic difference in treatment of certain objects, people, or groups in comparison to others [ISO 22989, 3.4.6]"@en ;
          rdfs:label "Bias"@en .


###  https://w3id.org/airo#BiasCorrection
airo:BiasCorrection rdf:type owl:Class ;
                    rdfs:subClassOf airo:Measure .


###  https://w3id.org/airo#BiasDetection
airo:BiasDetection rdf:type owl:Class ;
                   rdfs:subClassOf airo:Measure .


###  https://w3id.org/airo#BiasMonitoring
airo:BiasMonitoring rdf:type owl:Class ;
                    rdfs:subClassOf airo:Measure .


###  https://w3id.org/airo#BiasedOutput
airo:BiasedOutput rdf:type owl:Class ;
                  rdfs:subClassOf airo:Consequence .


###  https://w3id.org/airo#BiasedTestingDataSet
airo:BiasedTestingDataSet rdf:type owl:Class ;
                          rdfs:subClassOf airo:DataGovernanceRelatedRiskSource .


###  https://w3id.org/airo#BiasedTrainingDataSet
airo:BiasedTrainingDataSet rdf:type owl:Class ;
                           rdfs:subClassOf airo:DataGovernanceRelatedRiskSource .


