        <rdfs:domain rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Event"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#LikelihoodRate"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#hasRisk -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#hasRisk">
