        <rdfs:range rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Measure"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#controlsImpactMaterialisation -->


