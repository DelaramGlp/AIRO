        <rdfs:range rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Event"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#isRiskSource -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#isRiskSource">
