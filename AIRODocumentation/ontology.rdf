        <rdfs:domain rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiSystem"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiApproach"/>
