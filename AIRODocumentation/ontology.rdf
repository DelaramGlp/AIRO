    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiApproach -->


    <owl:Class rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiApproach">
        <rdfs:comment xml:lang="en">Refers to the main approaches and techniques used for development of AI systems.</rdfs:comment>
        <rdfs:label xml:lang="en">AI Approach</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiRisk -->


    <owl:Class rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiRisk">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Event"/>
        <rdfs:comment xml:lang="en">Indicates a risk (of harm) associated with an AI system.</rdfs:comment>
        <rdfs:label xml:lang="en">AI Risk</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiSystem -->


    <owl:Class rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AiSystem">
        <rdfs:comment xml:lang="en">Represents a software that is developed with one or more of the AI Approaches and can, for a given set of human-defined objectives, generate outputs such as content, predictions, recommendations, or decisions influencing the environments they interact with. [Source: AI Act, Art 3(1)]</rdfs:comment>
        <rdfs:label xml:lang="en">AI System</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AreaOfInfluence -->


    <owl:Class rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AreaOfInfluence">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Health"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#HumanBehaviour"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Safety"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Security"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Welfare"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:label xml:lang="en">Area of Influence</rdfs:label>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Attack -->


    <owl:Class rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Attack">
