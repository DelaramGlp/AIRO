        <rdfs:domain rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#AreaOfInfluence"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#Impact"/>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/delaram/ontologies/2021/10/airo#consequenceIsMaterialisedBy -->


    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo#consequenceIsMaterialisedBy">
