    <owl:Ontology rdf:about="http://www.semanticweb.org/delaram/ontologies/2021/10/airo">
        <terms:creator>Delaram Golpayegani</terms:creator>
        <terms:description xml:lang="en">The ontolgy represents AI risk concepts and relations based on the AI Act draft.</terms:description>
