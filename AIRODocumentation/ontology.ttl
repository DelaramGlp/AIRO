@prefix : <https://w3id.org/airo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/airo> .

<https://w3id.org/airo> rdf:type owl:Ontology ;
                         <http://purl.org/dc/terms/creator> "Delaram Golpayegani" ;
                         <http://purl.org/dc/terms/description> "The ontolgy represents AI risk concepts and relations based on the AI Act draft and ISO 31000 standard series."@en ;
                         <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                         <http://purl.org/dc/terms/publisher> "ADAPT Centre, Trinity College Dublin" ;
                         rdfs:label "AI Risk Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/airo#hasConsequence
:hasConsequence rdf:type owl:ObjectProperty ;
                rdfs:domain :Risk ;
                rdfs:range :RiskConsequence ;
                rdfs:label "has consequence"@en .


###  https://w3id.org/airo#hasExtentOfUseIndicator
:hasExtentOfUseIndicator rdf:type owl:ObjectProperty ;
                         rdfs:domain :AiSystem ;
                         rdfs:range :ExtentOfUse .


###  https://w3id.org/airo#hasImpact
:hasImpact rdf:type owl:ObjectProperty ;
           owl:inverseOf :isImpactOf ;
           rdfs:domain :RiskConsequence ;
           rdfs:range :Impact ;
           rdfs:label "has impact"@en .


###  https://w3id.org/airo#hasImpactOnArea
:hasImpactOnArea rdf:type owl:ObjectProperty ;
                 rdfs:domain :Impact ;
                 rdfs:range :AreaOfInfluence ;
                 rdfs:label "has impact on area"@en .


###  https://w3id.org/airo#hasImpactOnStakeholder
:hasImpactOnStakeholder rdf:type owl:ObjectProperty ;
                        rdfs:domain :Impact ;
                        rdfs:range :Stakeholder ;
                        rdfs:label "has impact on stakeholder"@en .


###  https://w3id.org/airo#hasIntendedPurpose
:hasIntendedPurpose rdf:type owl:ObjectProperty ;
                    owl:inverseOf :isIntendedPurposeOf ;
                    rdfs:domain :AiSystem ;
                    rdfs:range :IntendedPurpose ;
                    rdfs:label "Indicates the intended purpose of an AI system."@en ,
                               "has intended purpose"@en .


###  https://w3id.org/airo#hasLikelihood
:hasLikelihood rdf:type owl:ObjectProperty ;
               rdfs:domain :Event ;
               rdfs:range :LikelihoodRate ;
               rdfs:label "Indicates the probability of occurrence of an event."@en ,
                          "has likelihood"@en .


###  https://w3id.org/airo#hasPerformanceIndicator
:hasPerformanceIndicator rdf:type owl:ObjectProperty ;
                         rdfs:domain :AiSystem ;
                         rdfs:range :PerformaceOfAISystem .


###  https://w3id.org/airo#hasRisk
:hasRisk rdf:type owl:ObjectProperty ;
         owl:inverseOf :isRiskAssociatedWith ;
         rdfs:domain :AiSystemComponent ;
         rdfs:range :Risk ;
         rdfs:label "has risk"@en .


###  https://w3id.org/airo#hasRiskSource
:hasRiskSource rdf:type owl:ObjectProperty ;
               owl:inverseOf :isRiskSourceFor ;
               rdfs:domain :Risk ;
               rdfs:range :RiskSource ;
               rdfs:label "has risk source"@en .


###  https://w3id.org/airo#hasSeverity
:hasSeverity rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Impact
                                         :RiskConsequence
                                       )
                         ] ;
             rdfs:range :Severity ;
             rdfs:label "has severity"@en .


###  https://w3id.org/airo#hasStakeholder
:hasStakeholder rdf:type owl:ObjectProperty ;
                owl:inverseOf :isStakeholderOfSystem ;
                rdfs:domain :AiSystem ;
                rdfs:range :Stakeholder ;
                rdfs:comment "Indicates stakeholders of an AI system"@en ;
                rdfs:label "has stakeholder"@en .


###  https://w3id.org/airo#hasVulnerability
:hasVulnerability rdf:type owl:ObjectProperty ;
                  rdfs:domain :AiSystem ;
                  rdfs:range :vulnerability ;
                  rdfs:comment "Indicates vulnerabilities of an AI system."@en ;
                  rdfs:label "has vulnerability"@en .


###  https://w3id.org/airo#isConsequenceOf
:isConsequenceOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :RiskConsequence ;
                 rdfs:range :Risk ;
                 rdfs:label "is consequence of"@en .


###  https://w3id.org/airo#isFollowedByControl
:isFollowedByControl rdf:type owl:ObjectProperty ;
                     rdfs:domain :Measure ;
                     rdfs:range :Measure ;
                     rdfs:label "is followed by control"@en .


###  https://w3id.org/airo#isImpactOf
:isImpactOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Impact ;
            rdfs:range :RiskConsequence ;
            rdfs:label "is impact of"@en .


###  https://w3id.org/airo#isInVulnerablePosition
:isInVulnerablePosition rdf:type owl:ObjectProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Group
                                                    :Person
                                                  )
                                    ] ;
                        rdfs:range :VulnerablePosition ;
                        rdfs:label "is in vulnerable position"@en .


###  https://w3id.org/airo#isIntendedPurposeOf
:isIntendedPurposeOf rdf:type owl:ObjectProperty ;
                     rdfs:domain :IntendedPurpose ;
                     rdfs:range :AiSystem ;
                     rdfs:label "is intended purpose of"@en .


###  https://w3id.org/airo#isPartOfAiSystem
:isPartOfAiSystem rdf:type owl:ObjectProperty ;
                  rdfs:domain :AiSystemComponent ;
                  rdfs:range :AiSystem ;
                  rdfs:comment "Indicates that a component is part of an AI system."@en ;
                  rdfs:label "is part of ai system"@en .


###  https://w3id.org/airo#isPartOfControl
:isPartOfControl rdf:type owl:ObjectProperty ;
                 rdfs:label "is part of control"@en .


###  https://w3id.org/airo#isRiskAssociatedWith
:isRiskAssociatedWith rdf:type owl:ObjectProperty ;
                      rdfs:domain :Risk ;
                      rdfs:range :AiSystemComponent ;
                      rdfs:label "is risk associated with"@en .


###  https://w3id.org/airo#isRiskSourceFor
:isRiskSourceFor rdf:type owl:ObjectProperty ;
                 rdfs:domain :RiskSource ;
                 rdfs:range :Risk ;
                 rdfs:label "is risk source for"@en .


###  https://w3id.org/airo#isStakeholderOfSystem
:isStakeholderOfSystem rdf:type owl:ObjectProperty ;
                       rdfs:domain :Stakeholder ;
                       rdfs:range :AiSystem ;
                       rdfs:label "is stakeholder of system"@en .


###  https://w3id.org/airo#modifiesEvent
:modifiesEvent rdf:type owl:ObjectProperty ;
               rdfs:domain :Measure ;
               rdfs:range :Event ;
               rdfs:label "modifies event"@en .


###  https://w3id.org/airo#producesOutput
:producesOutput rdf:type owl:ObjectProperty ;
                rdfs:domain :AiSystem ;
                rdfs:range :Output ;
                rdfs:label "produces output"@en .


###  https://w3id.org/airo#usesTechnique
:usesTechnique rdf:type owl:ObjectProperty ;
               rdfs:domain :AiSystem ;
               rdfs:range :AiTechnique ;
               rdfs:comment "Indicates the AI techniques used in an AI system."@en ;
               rdfs:label "uses technique"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/airo#Accuracy
:Accuracy rdf:type owl:Class ;
          rdfs:subClassOf :Indicator .


###  https://w3id.org/airo#AdversarialAttack
:AdversarialAttack rdf:type owl:Class ;
                   rdfs:subClassOf :Cyberattack ;
                   rdfs:comment "inputs designed to cause the model to make a mistake"@en ;
                   rdfs:label "Adversarial Attack"@en .


###  https://w3id.org/airo#AiSystem
:AiSystem rdf:type owl:Class ;
          rdfs:comment "Represents a software that is developed with one or more of the AI Approaches and can, for a given set of human-defined objectives, generate outputs such as content, predictions, recommendations, or decisions influencing the environments they interact with. [Source: AI Act, Art 3(1)]"@en ;
          rdfs:label "AI System"@en .


###  https://w3id.org/airo#AiSystemComponent
:AiSystemComponent rdf:type owl:Class ;
                   rdfs:label "AI System Component"@en .


###  https://w3id.org/airo#AiTechnique
:AiTechnique rdf:type owl:Class ;
             rdfs:comment "Refers to the main approaches and techniques used for development of AI systems."@en ;
             rdfs:label "AI Technique"@en .


