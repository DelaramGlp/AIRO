@prefix : <https://w3id.org/airo#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@base <https://w3id.org/airo> .

<https://w3id.org/airo> rdf:type owl:Ontology ;
                         <http://purl.org/dc/terms/creator> "Delaram Golpayegani" ;
                         <http://purl.org/dc/terms/description> "The ontology represents AI risk concepts and relations based on the AI Act draft and ISO 31000 standard series."@en ;
                         <http://purl.org/dc/terms/license> <https://creativecommons.org/licenses/by/4.0/> ;
                         <http://purl.org/dc/terms/publisher> "ADAPT Centre, Trinity College Dublin" ;
                         rdfs:label "AI Risk Ontology"@en .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/creator
<http://purl.org/dc/terms/creator> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/description
<http://purl.org/dc/terms/description> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/license
<http://purl.org/dc/terms/license> rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/publisher
<http://purl.org/dc/terms/publisher> rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  https://w3id.org/airo#detectsEvent
:detectsEvent rdf:type owl:ObjectProperty ;
              rdfs:subPropertyOf :modifiesEvent ;
              rdfs:domain :DetectionControl ;
              rdfs:range :Event ;
              rdfs:label "detects event"@en .


###  https://w3id.org/airo#eliminatesEvent
:eliminatesEvent rdf:type owl:ObjectProperty ;
                 rdfs:subPropertyOf :modifiesEvent ;
                 rdfs:label "eliminates event"@en .


###  https://w3id.org/airo#hasComponent
:hasComponent rdf:type owl:ObjectProperty ;
              owl:inverseOf :isPartOfAISystem ;
              rdfs:domain :AISystem ;
              rdfs:range :AIComponent ;
              rdfs:label "has component"@en .


###  https://w3id.org/airo#hasConsequence
:hasConsequence rdf:type owl:ObjectProperty ;
                rdfs:domain :Risk ;
                rdfs:range :Consequence ;
                rdfs:label "has consequence"@en .


###  https://w3id.org/airo#hasExtentOfUseIndicator
:hasExtentOfUseIndicator rdf:type owl:ObjectProperty ;
                         rdfs:domain :AISystem ;
                         rdfs:range :ExtentOfUse .


###  https://w3id.org/airo#hasImpact
:hasImpact rdf:type owl:ObjectProperty ;
           owl:inverseOf :isImpactOf ;
           rdfs:domain :Consequence ;
           rdfs:range :Impact ;
           rdfs:label "has impact"@en .


###  https://w3id.org/airo#hasImpactOnArea
:hasImpactOnArea rdf:type owl:ObjectProperty ;
                 rdfs:domain :Impact ;
                 rdfs:range :Area ;
                 rdfs:label "has impact on area"@en .


###  https://w3id.org/airo#hasImpactOnStakeholder
:hasImpactOnStakeholder rdf:type owl:ObjectProperty ;
                        rdfs:domain :Impact ;
                        rdfs:range :AISubject ;
                        rdfs:label "has impact on stakeholder"@en .


###  https://w3id.org/airo#hasLikelihood
:hasLikelihood rdf:type owl:ObjectProperty ;
               rdfs:domain :Event ;
               rdfs:range :LikelihoodRate ;
               rdfs:label "Indicates the probability of occurrence of an event."@en ,
                          "has likelihood"@en .


###  https://w3id.org/airo#hasPerformanceIndicator
:hasPerformanceIndicator rdf:type owl:ObjectProperty ;
                         rdfs:domain :AISystem ;
                         rdfs:range :Performace .


###  https://w3id.org/airo#hasPurpose
:hasPurpose rdf:type owl:ObjectProperty ;
            owl:inverseOf :isPurposeOf ;
            rdfs:domain :AISystem ;
            rdfs:range :Purpose ;
            rdfs:label "Indicates the intended purpose of an AI system."@en ,
                       "has purpose"@en .


###  https://w3id.org/airo#hasRisk
:hasRisk rdf:type owl:ObjectProperty ;
         owl:inverseOf :isRiskAssociatedWith ;
         rdfs:range :Risk ;
         rdfs:label "has risk"@en .


###  https://w3id.org/airo#hasRiskSource
:hasRiskSource rdf:type owl:ObjectProperty ;
               owl:inverseOf :isRiskSourceFor ;
               rdfs:domain :Risk ;
               rdfs:range :Source ;
               rdfs:label "has risk source"@en .


###  https://w3id.org/airo#hasSeverity
:hasSeverity rdf:type owl:ObjectProperty ;
             rdfs:domain [ rdf:type owl:Class ;
                           owl:unionOf ( :Consequence
                                         :Impact
                                       )
                         ] ;
             rdfs:range :Severity ;
             rdfs:label "has severity"@en .


###  https://w3id.org/airo#hasStakeholder
:hasStakeholder rdf:type owl:ObjectProperty ;
                owl:inverseOf :isStakeholderOfSystem ;
                rdfs:domain :AISystem ;
                rdfs:range :Stakeholder ;
                rdfs:comment "Indicates stakeholders of an AI system"@en ;
                rdfs:label "has stakeholder"@en .


###  https://w3id.org/airo#hasVulnerability
:hasVulnerability rdf:type owl:ObjectProperty ;
                  rdfs:domain :AISystem ;
                  rdfs:range :vulnerability ;
                  rdfs:comment "Indicates vulnerabilities of an AI system."@en ;
                  rdfs:label "has vulnerability"@en .


###  https://w3id.org/airo#isConsequenceOf
:isConsequenceOf rdf:type owl:ObjectProperty ;
                 rdfs:domain :Consequence ;
                 rdfs:range :Risk ;
                 rdfs:label "is consequence of"@en .


###  https://w3id.org/airo#isFollowedByControl
:isFollowedByControl rdf:type owl:ObjectProperty ;
                     rdfs:domain :Measure ;
                     rdfs:range :Measure ;
                     rdfs:label "is followed by control"@en .


###  https://w3id.org/airo#isImpactOf
:isImpactOf rdf:type owl:ObjectProperty ;
            rdfs:domain :Impact ;
            rdfs:range :Consequence ;
            rdfs:label "is impact of"@en .


###  https://w3id.org/airo#isInVulnerablePosition
:isInVulnerablePosition rdf:type owl:ObjectProperty ;
                        rdfs:domain [ rdf:type owl:Class ;
                                      owl:unionOf ( :Group
                                                    :Person
                                                  )
                                    ] ;
                        rdfs:range :VulnerablePosition ;
                        rdfs:label "is in vulnerable position"@en .


###  https://w3id.org/airo#isPartOfAISystem
:isPartOfAISystem rdf:type owl:ObjectProperty ;
                  rdfs:domain :AIComponent ;
                  rdfs:range :AISystem ;
                  rdfs:comment "Indicates that a component is part of an AI system."@en ;
                  rdfs:label "is part of AI system"@en .


###  https://w3id.org/airo#isPartOfControl
:isPartOfControl rdf:type owl:ObjectProperty ;
                 rdfs:domain :PartialControl ;
                 rdfs:range :Measure ;
                 rdfs:label "is part of control"@en .


###  https://w3id.org/airo#isPurposeOf
:isPurposeOf rdf:type owl:ObjectProperty ;
             rdfs:domain :Purpose ;
             rdfs:range :AISystem ;
             rdfs:label "is purpose of"@en .


###  https://w3id.org/airo#isRiskAssociatedWith
:isRiskAssociatedWith rdf:type owl:ObjectProperty ;
                      rdfs:domain :Risk ;
                      rdfs:label "is risk associated with"@en .


###  https://w3id.org/airo#isRiskSourceFor
:isRiskSourceFor rdf:type owl:ObjectProperty ;
                 rdfs:domain :Source ;
                 rdfs:range :Risk ;
                 rdfs:label "is risk source for"@en .


###  https://w3id.org/airo#isStakeholderOfSystem
:isStakeholderOfSystem rdf:type owl:ObjectProperty ;
                       rdfs:domain :Stakeholder ;
                       rdfs:range :AISystem ;
                       rdfs:label "is stakeholder of system"@en .


###  https://w3id.org/airo#isUsedInIndustry
:isUsedInIndustry rdf:type owl:ObjectProperty ;
                  rdfs:comment "Indicates the industry (context) the AI system is intended to be used in."@en ;
                  rdfs:label "is used in industry"@en .


###  https://w3id.org/airo#mitigatesEvent
:mitigatesEvent rdf:type owl:ObjectProperty ;
                rdfs:subPropertyOf :modifiesEvent ;
                rdfs:label "mitigates event"@en .


###  https://w3id.org/airo#modifiesEvent
:modifiesEvent rdf:type owl:ObjectProperty ;
               rdfs:domain :Measure ;
               rdfs:range :Event ;
               rdfs:label "modifies event"@en .


###  https://w3id.org/airo#monitorsEvent
:monitorsEvent rdf:type owl:ObjectProperty ;
               rdfs:subPropertyOf :modifiesEvent ;
               rdfs:label "monitors event"@en .


###  https://w3id.org/airo#producesOutput
:producesOutput rdf:type owl:ObjectProperty ;
                rdfs:domain :AISystem ;
                rdfs:range :Output ;
                rdfs:label "produces output"@en .


###  https://w3id.org/airo#usesTechnique
:usesTechnique rdf:type owl:ObjectProperty ;
               rdfs:domain :AISystem ;
               rdfs:range :AITechnique ;
               rdfs:comment "Indicates the AI techniques used in an AI system."@en ;
               rdfs:label "uses technique"@en .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/airo#AIAuditor
:AIAuditor rdf:type owl:Class ;
           rdfs:subClassOf :AIPartner ;
           rdfs:comment "An AI auditor is an organization or entity that is concerned with the audit of organizations producing, providing or using AI systems, to assess conformance to standards, policies or legal requirements. [ISO 22989]"@en ;
           rdfs:label "AI Auditor"@en .


###  https://w3id.org/airo#AIComponent
:AIComponent rdf:type owl:Class ;
             rdfs:comment "Represents a functional element that constructs an AI system. [Source: ISO 22989]"@en ;
             rdfs:label "AI Component"@en .


###  https://w3id.org/airo#AICustomer
:AICustomer rdf:type owl:Class ;
            rdfs:subClassOf :Stakeholder ;
            rdfs:comment "An AI customer is an organization or entity that uses an AI product or service either directly or by its  provision to AI users. [Source: ISO 22989]"@en ;
            rdfs:label "AI Customer"@en .


###  https://w3id.org/airo#AIDeveloper
:AIDeveloper rdf:type owl:Class ;
             rdfs:subClassOf :AIProducer ;
             rdfs:comment "An AI developer is an organization or entity that is concerned with the development of AI services and products. [Source: ISO 22989]"@en ;
             rdfs:label "AI Developer"@en .


###  https://w3id.org/airo#AIEvaluator
:AIEvaluator rdf:type owl:Class ;
             rdfs:subClassOf :AIPartner ;
             rdfs:comment "An AI evaluator is an organization or entity that evaluates the performance of one or more AI systems. [Source: ISO 22989]"@en ;
             rdfs:label "AI Evaluator"@en .


###  https://w3id.org/airo#AIPartner
:AIPartner rdf:type owl:Class ;
           rdfs:subClassOf :Stakeholder ;
           rdfs:comment "An AI partner is an organization or entity that provides services in the context of AI. AI partners can  perform technical development of AI products or services, conduct testing and validation of AI products and services, audit AI usage, evaluate AI products or services and perform other tasks. Examples of AI partner types are discussed in the following subclauses. [Source: ISO 22989]"@en ;
           rdfs:label "AI Partner"@en .


###  https://w3id.org/airo#AIPlatformProvider
:AIPlatformProvider rdf:type owl:Class ;
                    rdfs:subClassOf :AIProvider ;
                    rdfs:comment "An AI platform provider is an organization or entity that provides services that enable other stakeholders to produce AI services or products. [Source: ISO 22989]"@en ;
                    rdfs:label "AI Platform Provider"@en .


###  https://w3id.org/airo#AIProducer
:AIProducer rdf:type owl:Class ;
            rdfs:subClassOf :Stakeholder ;
            rdfs:comment "An AI producer is an organization or entity that designs, develops, tests and deploys products or services that use one or more AI system. [Source: ISO 22989]"@en ;
            rdfs:label "AI Producer"@en .


###  https://w3id.org/airo#AIProductProvider
:AIProductProvider rdf:type owl:Class ;
                   rdfs:subClassOf :AIProvider ;
                   rdfs:comment "An AI product provider is an organization or entity that provides AI products either directly usable by an AI customer or user, or to be integrated into a system using AI along with non-AI components. [Source: ISO 22989]"@en ;
                   rdfs:label "AI Product Provider"@en .


###  https://w3id.org/airo#AIProvider
:AIProvider rdf:type owl:Class ;
            rdfs:subClassOf :Stakeholder ;
            rdfs:comment "An AI provider is an organization or entity that provides products or services that uses one or more AI systems. AI providers encompass AI platform providers and AI product or service providers. [Source: ISO 22989]"@en ;
            rdfs:label "AI Provider"@en .


###  https://w3id.org/airo#AIServiceProvider
:AIServiceProvider rdf:type owl:Class ;
                   rdfs:subClassOf :AIProvider ;
                   rdfs:label "AI Service Provider"@en ,
                              "An AI service provider is an organization or entity that provides AI services  either  directly usable by an AI customer or user, or to be integrated into a system using AI along with non-AI components. [Source: ISO 22989]"@en .


###  https://w3id.org/airo#AISubject
:AISubject rdf:type owl:Class ;
           rdfs:subClassOf :Stakeholder ;
           rdfs:comment "An AI subject is an organization or entity that is impacted by an AI system, service or product."@en ;
           rdfs:label "AI Subject"@en .


###  https://w3id.org/airo#AISystem
:AISystem rdf:type owl:Class ;
          rdfs:comment "Represents a software that is developed with one or more of the AI Approaches and can, for a given set of human-defined objectives, generate outputs such as content, predictions, recommendations, or decisions influencing the environments they interact with. [Source: AI Act, Art 3(1)]"@en ;
          rdfs:label "AI System"@en .


###  https://w3id.org/airo#AISystemAccuracy
:AISystemAccuracy rdf:type owl:Class ;
                  rdfs:subClassOf :Accuracy ;
                  rdfs:comment "degree to which a product or system provides the correct results with the needed degree of precision. [ISO 22989, 3.4.4]"@en ;
                  rdfs:label "AI System Accuracy"@en .


###  https://w3id.org/airo#AISystemIntegrator
:AISystemIntegrator rdf:type owl:Class ;
                    rdfs:subClassOf :AIPartner ;
                    rdfs:label "AI System Integrator"@en ,
                               "An AI system integrator is an organization or entity that is concerned with the integration of AI components into larger systems, potentially also including non-AI components. [Source: ISO 22989]"@en .


###  https://w3id.org/airo#AITechnique
:AITechnique rdf:type owl:Class ;
             rdfs:comment "Represents the main approaches and techniques used for development of AI systems."@en ;
             rdfs:label "AI Technique"@en .


###  https://w3id.org/airo#AIUser
:AIUser rdf:type owl:Class ;
        rdfs:subClassOf :AICustomer ;
        rdfs:comment "An AI user is an organization or entity that uses AI products or services. [Source: ISO 22989]"@en ;
        rdfs:label "AI User"@en .


###  https://w3id.org/airo#Accuracy
:Accuracy rdf:type owl:Class ;
          rdfs:subClassOf :AiIndicator .


###  https://w3id.org/airo#AdmissionTestAssessment
:AdmissionTestAssessment rdf:type owl:Class ;
                         rdfs:subClassOf :EducationRelatedAssessment ;
                         rdfs:label "Admission Test Assessment"@en .


###  https://w3id.org/airo#AdversarialAttack
:AdversarialAttack rdf:type owl:Class ;
                   rdfs:subClassOf :Attack ;
                   rdfs:comment "inputs designed to cause the model to make a mistake"@en ;
                   rdfs:label "Adversarial Attack"@en .


###  https://w3id.org/airo#AdvertisingVacancies
:AdvertisingVacancies rdf:type owl:Class ;
                      rdfs:subClassOf :MakingDecision ;
                      rdfs:label "Advertising Vacancies"@en .


###  https://w3id.org/airo#AiIndicator
:AiIndicator rdf:type owl:Class ;
             rdfs:subClassOf :Indicator ;
             rdfs:label "AI Indicator"@en .


###  https://w3id.org/airo#Area
:Area rdf:type owl:Class ;
      rdfs:comment "Represents areas that AI might have impact on."@en ;
      rdfs:label "Area"@en .


###  https://w3id.org/airo#Assessment
:Assessment rdf:type owl:Class ;
            rdfs:subClassOf :MakingDecision .


###  https://w3id.org/airo#Attack
:Attack rdf:type owl:Class ;
        rdfs:subClassOf :Source ;
        rdfs:label "Attack"@en .


###  https://w3id.org/airo#Authority
:Authority rdf:type owl:Class ;
           rdfs:subClassOf :Stakeholder ;
           rdfs:comment "Authorities are organizations or entities that can have an impact on an AI system, service or product."@en ;
           rdfs:label "Authority"@en .


###  https://w3id.org/airo#AutomatedControl
:AutomatedControl rdf:type owl:Class ;
                  rdfs:subClassOf :Measure ;
                  owl:disjointWith :ManualControl ;
                  rdfs:comment "Represents controls that do not involve human intervention."@en ;
                  rdfs:label "Automated Control"@en .


###  https://w3id.org/airo#Automotive
:Automotive rdf:type owl:Class ;
            rdfs:subClassOf :RegulatedIndustry ;
            rdfs:label "Automotive"@en .


###  https://w3id.org/airo#Aviation
:Aviation rdf:type owl:Class ;
          rdfs:subClassOf :RegulatedIndustry ;
          rdfs:label "Aviation"@en .


###  https://w3id.org/airo#Bias
:Bias rdf:type owl:Class ;
      rdfs:subClassOf :BiasRelatedConsequence ;
      rdfs:comment "Systematic difference in treatment of certain objects, people, or groups in comparison to others [ISO 22989]"@en ;
      rdfs:label "Bias"@en .


###  https://w3id.org/airo#BiasDetection
:BiasDetection rdf:type owl:Class ;
               rdfs:subClassOf :DetectionControl ;
               rdfs:label "Bias Detection"@en .


###  https://w3id.org/airo#BiasMonitoring
:BiasMonitoring rdf:type owl:Class ;
                rdfs:subClassOf :MonitoringControl ;
                rdfs:label "Bias Monitoring"@en .


###  https://w3id.org/airo#BiasRelatedConsequence
:BiasRelatedConsequence rdf:type owl:Class ;
                        rdfs:subClassOf :Consequence ;
                        rdfs:comment "Represents consequences related to bias."@en ;
                        rdfs:label "Bias Related Consequence"@en .


###  https://w3id.org/airo#BiasedTestingDataSet
:BiasedTestingDataSet rdf:type owl:Class ;
                      rdfs:subClassOf :TestingDataRelatedSource .


###  https://w3id.org/airo#BiasedTrainingDataSet
:BiasedTrainingDataSet rdf:type owl:Class ;
                       rdfs:subClassOf :TrainingDataRelatedSource .


###  https://w3id.org/airo#BiasedValidationDataSet
:BiasedValidationDataSet rdf:type owl:Class ;
                         rdfs:subClassOf :ValidationDataRelatedSource .


###  https://w3id.org/airo#BiometricIdnetification
:BiometricIdnetification rdf:type owl:Class ;
                         rdfs:subClassOf :MakingDecision ;
                         rdfs:label "Biometric Identification"@en .


###  https://w3id.org/airo#Child
:Child rdf:type owl:Class ;
       rdfs:subClassOf :Person ;
       rdfs:label "Child"@en .


###  https://w3id.org/airo#ComputationVerifier
:ComputationVerifier rdf:type owl:Class ;
                     rdfs:subClassOf :AIDeveloper ;
                     rdfs:comment "The entity that verifies that a computation is being executed as designed. [Source: ISO 22989]"@en ;
                     rdfs:label "Computation Verifier"@en .


###  https://w3id.org/airo#Consequence
:Consequence rdf:type owl:Class ;
             rdfs:subClassOf :Event ;
             rdfs:comment "Outcome of an event affecting objectives. [Source: ISO 31000, 3.6]"@en ;
             rdfs:label "Consequence"@en .


###  https://w3id.org/airo#Content
:Content rdf:type owl:Class ;
         rdfs:subClassOf :Output ;
